<MOML version="1.1.5" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.applusform.com/xsd/moml_ui_1.1.5.xsd">

	<UILAYOUT portrait="320,460" landscape="320,460">
		<WEBVIEW id="web" layout="0,{userVariable.weby},320,{userVariable.webh}" src="{caller.homePage}" homePage="http://m.mafra.go.kr/bbs/Popup_list.jsp?menu_gubun=menu_4" controls="none">
			<LINKMAP baseUrl="">
				<LINK url="http://" cmd="function.root.openWebbrowser" />
			</LINKMAP>
		</WEBVIEW>
		<LABEL id="navigationTitle" themeId="themeNavigationTitle" text="알림판" onClick="">
			<BUTTON themeId="themeMenuBtn" />
		</LABEL>
		<WINDOW id="toolbar" layout="0,420,320,40" align="linear:horizontal|vCenter" defaultImg="/res/webbg.png">
			<BUTTON id="back" layout="30,30" margin="10,0,10,0" defaultImg="/res/webback.png" onClick="web.back" />
			<BUTTON id="forward" layout="30,30" margin="10,0,10,0" defaultImg="/res/webfor.png" onClick="web.forward" />
			<BUTTON layout="30,30" margin="10,0,10,0" defaultImg="/res/webrefresh.png" onClick="web.refresh" />
			<BUTTON layout="30,30" margin="10,0,10,0" defaultImg="/res/webhome.png" onClick="web.clear, web.src=web.homePage" />
		</WINDOW>
	</UILAYOUT>

	<FUNCTION id="updateToolbar">
		<CMD condition="web.canBack" cmd="back.defaultImg='/res/webback.png'" elseCmd="back.defaultImg='/res/webbackgray.png'"/>
		<CMD condition="web.canForward" cmd="forward.defaultImg='/res/webfor.png'" elseCmd="forward.defaultImg='/res/webforgray.png'"/>
	</FUNCTION>
</MOML>