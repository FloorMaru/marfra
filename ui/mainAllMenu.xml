<MOML version="1.1.5" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.applusform.com/xsd/moml_ui_1.1.5.xsd">

	<FUNCTION id="hideAni(item)">
		<CMD cmd="animation.flyOut('allMenu', -320, -460, 960, 1380, 'easeOut1', 500, '')" />
		<CMD cmd="userVariable.selectedItem = item" />
		<CMD cmd="document.document.navigation.selectedItem=userVariable.selectedItem" delay="500" />
	</FUNCTION>

	<UILAYOUT portrait="320,460" landscape="320,460">
		<WINDOW id="allMenu" layout="0,0,320,460" align="relative" defaultImg="#000000" visible="visible" scroll="vertical" scrollBarVisible="true" onClick="'none'">
			<CONTAINER id="info" layout="5,45,150,170" src="news.xml" enabled="false" />
			<BUTTON layout="5,45,150,170" text="최신뉴스" textShadow="#333333" textColor="#ffffff" fontSize="16" defaultImg="#88000000" onClick="function.hideAni('news')" />

			<CONTAINER layout="160,45,150,170" src="notice.xml" enabled="false" />
			<BUTTON layout="160,45,150,170" text="공지공고" textShadow="#333333" textColor="#ffffff" fontSize="16" defaultImg="#88000000" onClick="function.hideAni('notice')" />

			<CONTAINER id="map" layout="5,220,150,170" src="explain.xml" enabled="false" />
			<BUTTON layout="5,220,150,170" text="해명/설명" textShadow="#333333" textColor="#ffffff" fontSize="16" defaultImg="#88000000" onClick="function.hideAni('explain')" />

			<CONTAINER layout="160,220,150,170" src="incruit.xml" enabled="false" />
			<BUTTON layout="160,220,150,170" text="채용정보" textShadow="#333333" textColor="#ffffff" fontSize="16" defaultImg="#88000000" onClick="function.hideAni('incruit')" />

			<CONTAINER layout="5,395,150,170" src="alert.xml" enabled="false" />
			<BUTTON layout="5,395,150,170" text="알림판" textShadow="#333333" textColor="#ffffff" fontSize="16" defaultImg="#88000000" onClick="function.hideAni('alert')" />

			<CONTAINER layout="160,395,150,170" src="stats.xml" enabled="false" />
			<BUTTON layout="160,395,150,170" text="통계" textShadow="#333333" textColor="#ffffff" fontSize="16" defaultImg="#88000000" onClick="function.hideAni('stats')" />

			<CONTAINER layout="5,570,150,170" src="price.xml" enabled="false" />
			<BUTTON layout="5,570,150,170" text="농식품가격" textShadow="#333333" textColor="#ffffff" fontSize="16" defaultImg="#88000000" onClick="function.hideAni('price')" />

			<CONTAINER layout="160,570,150,170" src="basket.xml" enabled="false" />
			<BUTTON layout="160,570,150,170" text="안심장보기" textShadow="#333333" textColor="#ffffff" fontSize="16" defaultImg="#88000000" onClick="function.hideAni('basket')" />

			<CONTAINER layout="5,745,150,170" src="blog.xml" enabled="false" />
			<BUTTON layout="5,745,150,170" text="블로그" textShadow="#333333" textColor="#ffffff" fontSize="16" defaultImg="#88000000" onClick="function.hideAni('blog')" />

			<CONTAINER layout="160,745,150,170" src="facebook.xml" enabled="false" />
			<BUTTON layout="160,745,150,170" text="페이스북" textShadow="#333333" textColor="#ffffff" fontSize="16" defaultImg="#88000000" onClick="function.hideAni('facebook')" />
		</WINDOW>
		<BUTTON layout="280,0,40,40" text="X" textColor="#ffffff" defaultImg="#000000" onClick="document.document.navigation.selectedItem=userVariable.selectedItem" />
	</UILAYOUT>
</MOML>